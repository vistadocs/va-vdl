\par {\listtext\pard\plain\ltrpar \s34 \rtlch\fcs1 \ab\af37 \ltrch\fcs0 \f37\cf1\lang1024\langfe1024\noproof\insrsid9708025\charrsid12989380 \loch\af37\dbch\af0\hich\f37 \'95\tab}
